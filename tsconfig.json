{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"allowJs": false,
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true,
		"outDir": "./types",
		"strict": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true
	},
	"exclude": ["test"]
}